<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.admin.travellog.TravelListActivity">

    <TextView
        android:id="@+id/travelList"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:background="@color/colorAccent"
        android:gravity="center_vertical"
        android:text="여행목록"
        android:textAlignment="center"
        android:textSize="20sp" />

    <ListView
        android:id="@+id/travel_lv"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="70dp" />

    <TextView
        android:id="@+id/travel_notify_tv"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:text="트래킹 이력이 존재하지 않습니다!"
        android:textSize="22sp" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/travel_fab"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_margin="16dp"
        android:src="@drawable/ic_tracking"
        app:backgroundTint="#FFFFFF" />


</RelativeLayout>
